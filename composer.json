{
    "name": "mez/queuejitsu-scheduler",
    "description": "QueueJitsu Scheduler",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Meredith",
            "email": "martin@sourceguru.net"
        }
    ],
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "stickee/*": "source",
            "mez/*": "source",
            "*": "dist"
        }
    },
    "require": {
        "php": "^7.1",
        "mez/queuejitsu": "^0.1 || ^2.0 || ^3.0.1",
        "mtdowling/cron-expression": "^1.2",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.4",
        "jakub-onderka/php-parallel-lint": "^0.9.2 || ^1.0.0",
        "nikic/php-parser": "^3.1",
        "phpmd/phpmd": "^2.6",
        "phpro/grumphp": "^0.11.6 || ^0.15.0 || ^0.16.0",
        "phpstan/phpstan": "^0.8.3 || ^0.9.0",
        "povils/phpmnd": "^1.1 || ^2.0",
        "predis/predis": "^1.1",
        "sebastian/phpcpd": "^3.0 || ^4.0",
        "sensiolabs/security-checker": "^4.1 || ^6.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "QueueJitsu\\Scheduler\\": [
                "src"
            ]
        }
    },
    "extra": {
        "zf": {
            "config-provider": "QueueJitsu\\Scheduler\\ConfigProvider"
        }
    }
}
